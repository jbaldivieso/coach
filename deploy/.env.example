# Coach Production Environment
# Copy this to .env and customize

DJANGO_SETTINGS_MODULE=config.settings
DJANGO_DEBUG=False
DJANGO_SECRET_KEY=generate-a-secure-key-here
DJANGO_ALLOWED_HOSTS=raspberrypi.local,192.168.1.xxx

# These should match ALLOWED_HOSTS for session auth to work
CORS_ALLOWED_ORIGINS=http://raspberrypi.local:8000,http://192.168.1.xxx:8000
CSRF_TRUSTED_ORIGINS=http://raspberrypi.local:8000,http://192.168.1.xxx:8000
